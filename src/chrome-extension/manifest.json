{
  "name": "Syntaxer",
  "version": "0.3",
  "description": "Syntaxer",
  "content_scripts": [{
        "matches": ["http://*/*.js*",
                    "file://*/*.js*",
                    "http://*/*.css*",
                    "file://*/*.css*"
        ],
        "js": ["prettify/prettify.js",
               "prettify/lang/lang-css.js",
               "beautify.js",
               "syntaxer-chrome.js"
